                <TITLE>
                    <P>Pilistvere Hooldekodu ehitustööd</P>
                </TITLE>
                <REFERENCE_NUMBER>215483</REFERENCE_NUMBER>
                <CPV_MAIN>
                    <CPV_CODE CODE="45232141"/>
                </CPV_MAIN>
                <TYPE_CONTRACT CTYPE="WORKS"/>
                <OBJECT_DESCR>
                    <CPV_ADDITIONAL>
                        <CPV_CODE CODE="45232141"/>
                    </CPV_ADDITIONAL>
